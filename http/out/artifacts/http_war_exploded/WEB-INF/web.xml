<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LonginServelt</servlet-name>
        <servlet-class>com.lrx.http.servlet.LonginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LonginServelt</servlet-name>
        <url-pattern>/longin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Test302</servlet-name>
        <servlet-class>com.lrx.http.servlet.Test302</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test302</servlet-name>
        <url-pattern>/t302</url-pattern>
    </servlet-mapping>
</web-app>